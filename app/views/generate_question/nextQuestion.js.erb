if('<%= @lastQuestion %>' == 'false') {
	$("#question").replaceWith("<%= escape_javascript(render(:partial => "questions"))%>")
} else {
	if('<%= @trainMode %>' == 'true') {
		if('<%= @acceptResult %>' == 'true') {
			$("#question").replaceWith("<%= escape_javascript(render(:partial => "generateGifts"))%>")
		} else {
			$("#question").replaceWith("<%= escape_javascript(render(:partial => "discardResult"))%>")
		}
		
	} else {
		$("#question").replaceWith("<%= escape_javascript(render(:partial => "result"))%>")
	}
	
}